local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Mini store by jngames V1", "Ocean")
 
--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Customer Apples")

MainSection:NewButton("customer 1 20 apples", "20 apples", function()
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = workspace.Areas:FindFirstChild("1777231006").Building.TomatoTable,
    [2] = "Customer_1",
    [3] = 20
}

game:GetService("ReplicatedStorage").NPC.Events.NpcGetToTargetRE:FireServer(unpack(args))
end)

MainSection:NewButton("customer 2 20 apples", "20 apples", function()
    -- Script generated by SimpleSpy - credits to exx#9394
    
    local args = {
        [1] = workspace.Areas:FindFirstChild("1777231006").Building.TomatoTable,
        [2] = "Customer_2",
        [3] = 20
    }
    
    game:GetService("ReplicatedStorage").NPC.Events.NpcGetToTargetRE:FireServer(unpack(args))
    end)

    MainSection:NewButton("customer 3 20 apples", "20 apples", function()
        -- Script generated by SimpleSpy - credits to exx#9394
        
        local args = {
            [1] = workspace.Areas:FindFirstChild("1777231006").Building.TomatoTable,
            [2] = "Customer_3",
            [3] = 20
        }
        
        game:GetService("ReplicatedStorage").NPC.Events.NpcGetToTargetRE:FireServer(unpack(args))
        end)
