--Fun
local Fun = Window:NewTab("buy coin box")
local FunSection = Fun:NewSection("buy coin box")

FunSection:NewButton("Buy coin box 3", "10k coins", function()
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "CoinBox",
    [2] = 3
}

game:GetService("ReplicatedStorage").Events.Lottery.BuyBoxRF:InvokeServer(unpack(args))
end)

FunSection:NewButton("Buy coin box 1", "10k coins", function()
    -- Script generated by SimpleSpy - credits to exx#9394
    
    local args = {
        [1] = "CoinBox",
        [2] = 1
    }
    
    game:GetService("ReplicatedStorage").Events.Lottery.BuyBoxRF:InvokeServer(unpack(args))
    end)
    
